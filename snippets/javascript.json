{
    "reducer": {
        "prefix": "reducer", 
        "body": [
            "function ${name}(state = initialState, action) {",
            "\tswitch (action.type) {",
            "\t\tcase ${ACTION_NAME}:",
            "\t\t\treturn Object.assign({}, state, {",
            "\t\t\t\tvisibilityFilter: action.filter",
            "\t\t\t})",
            "\t\tdefault:",
            "\t\t\treturn state",
            "\t}",
            "}"
        ]
    }
    "container": {
        "prefix": "container",
        "body": [
            "import { connect } from 'react-redux'",
            "import ${component} from '${path}'",
            "",
            "const mapStateToProps = (state, ownProps) => {",
            "\treturn {",
            "\t\t${propName}: ${prop}",
            "\t}",
            "}",
            "",
            "const mapDispatchToProps = (state, ownProps) => {",
            "\treturn {",
            "\t\t${propName}: () => {",
            "\t\t\tdispatch(${value}(ownProps))",
            "\t\t}",
            "\t}",
            "}",
            "const ${containerName} = connect(",
            "\t${mapStateToProps,}",
            "\t${mapDispatchToProps}",
            ")(${component})",
            "",
            "export default ${containerName}"
        ],
        "description" : "Redux container"
    },
    "mapStateToProps": {
        "prefix": "mstp",
        "body": [
            "const mapStateToProps = (state, ownProps) => {",
            "\treturn {",
            "\t\t${propName}: ${prop}",
            "\t}",
            "}"
        ]
    },
    "mapDispatchToProps": {
        "prefix": "mdtp",
        "body": [
            "const mapDispatchToProps = (state, ownProps) => {",
            "\treturn {",
            "\t\t${propName}: () => {",
            "\t\t\tdispatch(${value}(ownProps))",
            "\t\t}",
            "\t}",
            "}"
        ]
    },
    
    "connect": {
        "prefix": "connect",
        "body": [
            "const ${containerName} = connect(",
            "\t${mapStateToProps,}",
            "\t${mapDispatchToProps}",
            ")(${component})",
            "",
            "export default ${containerName}"
        ]
    }
}